{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom","es2016"],                     
      "allowJs": true,
      "outDir": "dist",
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "declaration": true,
      "downlevelIteration": true,
      "jsx": "react"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}